<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://com.ejie.x43f/X43FNSHF/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:x43f="http://com.ejie.x43f/X43FNSHF/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="ejgvDocument">
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="1" name="header" type="x43f:header"/>
      <xs:element maxOccurs="1" minOccurs="1" name="body" type="x43f:body"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="header">
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="1" name="type" type="xs:string">
        <xs:annotation>
          <xs:documentation>Para firmas XAdES, su
                        valor es "xades".             Para
                        firmas PKCS7 su valor es
                        "COMPLETE_SIGNATURE"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="placement" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="format" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="isConservable" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="isIdazkiDesktop" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="documentIsRequired" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="1" name="version" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="flags" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="tsaSerialNumber" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="body">
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="sign" type="xs:string">
        <xs:annotation>
          <xs:documentation>Este campo es para las firmas XAdES</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="simpleSignature" type="xs:string">
        <xs:annotation>
          <xs:documentation>Este campo es para firmas
                        completas PKCS7</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="signatureTimestamp" type="xs:string">
        <xs:annotation>
          <xs:documentation>Este campo es para firmas
                        completas PKCS7</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="signerCertificateRevocationStatus" type="xs:string">
        <xs:annotation>
          <xs:documentation>Este campo es para firmas
                        completas PKCS7</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ejgvDocumentType">
    <xs:sequence>
